<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE Plugin PUBLIC "sailpoint.dtd" "sailpoint.dtd">
<Plugin certificationLevel="None" displayName="Sailpoint VAADIN plugin" minSystemVersion="8.0" name="SailPointVaadinPlugin"
        version="0.0.1">
    <Attributes>
        <Map>
            <entry key="fullPage">
                <value>
                    <FullPage title="Sailpoint VAADIN plugin UI"/>
                </value>
            </entry>
            <entry key="snippets">
                <value>
                    <List>
                        <Snippet regexPattern=".*home.*" rightRequired="">
                            <Scripts>
                                <String>ui/widget/widget.js</String>
                            </Scripts>
                        </Snippet>
                    </List>
                </value>
            </entry>
        </Map>
    </Attributes>
</Plugin>

